URxvt*depth: 32
URxvt*background: [90]#000000
URxvt*foreground: #FFFFFF

#URxvt*font: xft:Meslo LG M:regular:pixelsize=20:antialias=true
#URxvt*boldFont: xft:Meslo LG M:bold:pixelsize=20:antialias=true
#URxvt*italicFont: xft:Meslo LG M:regular:italic:pixelsize=20:antialias=true
#URxvt*boldItalicFont: xft:Meslo LG M:bold:italic:pixelsize=20:antialias=true

URxvt*font: xft:SF Mono:medium:pixelsize=18:antialias=true
URxvt*boldFont: xft:SF Mono:bold:pixelsize=18:antialias=true
URxvt*italicFont: xft:SF Mono:medium:italic:pixelsize=18:antialias=true
URxvt*boldItalicFont: xft:SF Mono:bold:italic:pixelsize=18:antialias=true

URxvt*letterSpace: -1
URxvt*lineSpace:    1.618

!URxvt*font: xft:Hack:regular:pixelsize=22:antialias=true
!URxvt*boldFont: xft:Hack:bold:pixelsize=22:antialias=true
!URxvt*italicFont: xft:Hack:regular:italic:pixelsize=22:antialias=true
!URxvt*boldItalicFont: xft:Hack:bold:italic:pixelsize=22:antialias=true
!URxvt*letterSpace: -1




! scrollbar style - rxvt (default), 
! plain (compact), NeXT, or xtermn
URxvt.scrollstyle: plain
URxvt.scrollBar_right: True



! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0


#URxvt.letterSpace: -1

! Disable printing the terminal contents when pressing PrintScreen.
URxvt.print-pipe: "cat > /dev/null"


#URxvt.perl-ext-common: default,selection-to-clipboard,tabbed
URxvt.perl-ext-common: default,selection-to-clipboard,tabbed

URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

#URxvt.matcher.rend.0: Uline Bold fg5

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0
URxvt.tabbed.tabbar-bg: -1


!URxvt*termName: rxvt
